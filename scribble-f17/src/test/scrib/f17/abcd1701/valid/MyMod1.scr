module f17.abcd1701.valid.MyMod1;


global protocol Proto1(role C, role A, role S)
{
	rec X
	{
		choice at C
		{
			more() from C to A;
			continue X;
		}
		or
		{
			done() from C to S;	
			done() from C to A;		
		}
	}
}
