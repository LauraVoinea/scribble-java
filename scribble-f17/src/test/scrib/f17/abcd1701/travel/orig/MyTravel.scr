//$ bin/scribblec-f17.sh scribble-f17/src/test/scrib/f17/abcd1701/travel/orig/MyTravel.scr -f17 Travel

module f17.abcd1701.travel.orig.MyTravel;

type <java> "java.lang.String" from "rt.jar" as Str;
type <java> "java.lang.Integer" from "rt.jar" as Int;


global protocol Travel(role C, role A, role S)
{
	choice at C
	{
		query(Str) from C to A;
		quote(Int) from A to C;
		do Travel(C, A, S);
	}
	or
	{
		pay(Str) from C to S;
		confirm(Int) from S to C;
		accpt(Int) from C to A;    // Int should same val as the prev
	}
	/*or  // Unfinished role *safety* error -- if this case commented, then role progress *liveness* error (safety checked first, then liveness)
	{
		reject() from C to A;
	}*/
}
