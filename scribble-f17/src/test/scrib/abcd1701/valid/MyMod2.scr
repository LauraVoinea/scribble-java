module demo.fase17.abcd1701.valid.MyMod2;


explicit global protocol Proto1(role C, role A, role S)
{
	connect C to A;
	rec X
	{
		choice at C
		{
			more() from C to A;
			continue X;
		}
		or
		{
			done() connect C to S;	
			done() from C to A;		
		}
	}
}
