module good.extensions.assertions.ComplexMessageFlow;


type <java> "java.lang.Integer" from "rt.jar" as Integer;
type <java> "java.lang.Long" from "rt.jar" as Long;


// Check the message flow
global protocol Prot1(role A, role B, role C, role D)
{	
	MSG1(x:Integer, y:Integer) from A to B;  @(x > 10);
	
	MSG1(x, y) from B to C;
	
	MSG1(z:Integer) from C to D;  @(z < x);
}
